{
    "files.autoSave": "afterDelay",
    "editor.fontFamily": "'Maple Mono', Hack, 'Input Mono Condensed', 'CommitMono Nerd Font Mono', 'Fira Code', Consolas, 'Courier New', monospace",
    "javascript.format.semicolons": "insert",
    "window.titleBarStyle": "custom",
    "chatgpt.lang": "en",
    "symbols.hidesExplorerArrows": false,
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.tsx": "${capture}.ts",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
        "*.sqlite": "${capture}.${extname}-*",
        "*.db": "${capture}.${extname}-*",
        "*.sqlite3": "${capture}.${extname}-*",
        "*.db3": "${capture}.${extname}-*",
        "*.sdb": "${capture}.${extname}-*",
        "*.s3db": "${capture}.${extname}-*"
    },
    "python.createEnvironment.trigger": "off",
    "editor.fontLigatures": true,
    "http.proxyAuthorization": null,
    "cSpell.userWords": [
        "smth"
    ],
    "svelte.enable-ts-plugin": true,
    "editor.wordWrap": "on",
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "git.openRepositoryInParentFolders": "never",
    "editor.cursorStyle": "block",
    "workbench.activityBar.location": "hidden",
    "workbench.statusBar.visible": false,
    "tabnine.experimentalAutoImports": true,
    "tabnine.codeLensEnabled": false,
    "supermaven.enableFixWithSupermaven": true,
    "editor.renderControlCharacters": false,
    "editor.stickyScroll.enabled": false,
    "breadcrumbs.enabled": false,
    "editor.renderWhitespace": "none",
    "workbench.iconTheme": null,
    "editor.minimap.enabled": false,
    "terminal.integrated.fontSize": 16,
    "workbench.editor.empty.hint": "hidden",
    "editor.smoothScrolling": true,
    "supermaven.otherWarning": false,
    "workbench.colorCustomizations": {
        "terminal.background": "#00000000"
    },
    "workbench.settings.applyToAllProfiles": [
        "workbench.colorCustomizations"
    ],
    "glassit.alpha": 220,
    "workbench.editor.editorActionsLocation": "titleBar",
    "catppuccin-noctis-icons.hidesExplorerArrows": false,
    "editor.lineNumbers": "relative",
    "window.commandCenter": false,
    "workbench.layoutControl.enabled": false,
    "workbench.sideBar.location": "right",
    "window.zoomLevel": 0.2,
    "editor.accessibilitySupport": "off",
    "workbench.startupEditor": "none",
    "workbench.colorTheme": "Darcula Dark Theme",
    "editor.fontSize": 16,
    "editor.cursorBlinking": "phase",
    "window.menuBarVisibility": "compact"
}